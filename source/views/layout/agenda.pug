  section#agenda
    nav.navbar-mobile
      a.room-101(href='#101') 101
      a.room-201(href='#201') 201
      a.back-top(href='#agenda')
        i.fa.fa-arrow-up
    .container
      .row
        .col-md-12.date
          - var day = 0;
          each rooms, index in agenda
            - day = day + 1;
            if (day == 1)
              input(type='radio' name='agenda-toggle' id=('day-'+day) value=('day-'+day) checked)
            else
              input(type='radio' name='agenda-toggle' id=('day-'+day) value=('day-'+day))
            label(for='day-'+day) #{index}

          - day = 0;
          each rooms, index in agenda
            - day = day + 1;
            each agendaContents, agendaIndex in rooms
              .col-sm-6(class=('day-'+day))
                h2(id=agendaIndex) #{agendaIndex}
                each content, contentIndex in agendaContents
                  ul.timeline
                    li.event(data-date=content.time)
                      if (content.title)
                        h3 #{content.title}
                        p
                          | #{content.description}
                      else
                        h3 TDB
                        p
                          | ---


